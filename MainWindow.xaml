<Window x:Class="Power_BI_endpoint_reader.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Power_BI_endpoint_reader"
        mc:Ignorable="d"
        Title="Endpoint reader" Height="140" Width="300" MinHeight="140" MinWidth="300">
    <Grid>
        <TextBox x:Name="Server" Text="Server" TextWrapping="Wrap" VerticalAlignment="Top" Height="20" MaxLength="100" Margin="10,26,10,0"/>
        <TextBox x:Name="Database" Margin="10,73,10,0" Text="Database" TextWrapping="Wrap" VerticalAlignment="Top" Height="20" MaxLength="100"/>
        <Label Content="Server" Margin="10,0,10,0" VerticalAlignment="Top"/>
        <Label Content="Database" Margin="10,47,10,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
